{
  "name": "all-around-keyboard",
  "version": "0.0.1",
  "description": "Round piano keyboards with Web Components + d3.js",
  "private": true,
  "main": "dist/all-around-keyboard.js",
  "jsnext:main": "src/main.js",
  "minified:main": "dist/all-around-keyboard.min.js",
  "scripts": {
    "eslint": "eslint src",
    "docs": "cp $npm_package_main docs/examples/",
    "build": "npm-run-all transpile minify docs",
    "transpile": "rollup -c rollup.config.js",
    "minify": "uglifyjs $npm_package_main -cm -o $npm_package_minified_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_minified_main}.map",
    "watch": "chokidar 'src/**' -c 'rollup -c'"
  },
  "dependencies": {
    "d3-color": "^1.0.1",
    "d3-interpolate": "^1.1.1",
    "d3-scale": "^1.0.3",
    "d3-selection": "^1.0.2",
    "d3-shape": "^1.0.3",
    "d3-svg": "^0.2.0",
    "d3-transition": "^1.0.3",
    "hammer-timejs": "^1.1.0",
    "hammerjs": "^2.0.8",
    "js-combinatorics": "^0.5.2",
    "katex": "^0.6.0",
    "kefir": "^3.7.1",
    "kefir-observable-selector": "^0.2.1",
    "mathjs": "^3.5.3",
    "skatejs": "4.5.2",
    "skatejs-web-components": "micahscopes/web-components#newer-dependencies",
    "xstream": "^10.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint-config-es2015": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "npm-run-all": "^4.0.0",
    "rollup": "^0.31.2",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "uglify-js": "^2.7.3"
  },
  "license": "MIT"
}
